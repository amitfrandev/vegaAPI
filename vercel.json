{
  "functions": {
    "api/index.js": {
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/data/img-source/(.*)",
      "dest": "/api/data/img-source/$1",
      "headers": {
        "Cache-Control": "public, max-age=86400"
      }
    }
  ],
  "build": {
    "env": {
      "NODE_ENV": "production"
    }
  }
} 